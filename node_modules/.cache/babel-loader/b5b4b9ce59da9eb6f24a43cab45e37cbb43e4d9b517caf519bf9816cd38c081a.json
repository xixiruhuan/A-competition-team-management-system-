{"ast":null,"code":"export default {\n  name: \"Formwork\",\n  data() {\n    return {\n      value1: null,\n      tableData: [],\n      dialogFormVisible: false,\n      form: {},\n      total: 0,\n      pageNum: 1,\n      pageSize: 5,\n      name: \"\",\n      content: '',\n      viewDialogVis: false,\n      chart: null // echarts图表实例\n    };\n  },\n  created() {\n    //请求分页数据\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"http://localhost:9090/formwork/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = res.total;\n      });\n    },\n    view(content) {\n      this.content = content;\n      this.viewDialogVis = true;\n    },\n    handleAdd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handleUpdate(row) {\n      this.dialogFormVisible = true;\n      this.form = row;\n    },\n    save() {\n      this.request.post(\"http://localhost:9090/formwork\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"保存成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n    del(id) {\n      this.request.delete(\"http://localhost:9090/formwork/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    exp() {\n      window.open(\"http://localhost:9090/user/export\");\n    }\n  }\n};","map":{"version":3,"names":["name","data","value1","tableData","dialogFormVisible","form","total","pageNum","pageSize","content","viewDialogVis","chart","created","load","methods","request","get","params","then","res","console","log","view","handleAdd","handleUpdate","row","save","post","$message","success","error","del","id","delete","handleSizeChange","handleCurrentChange","exp","window","open"],"sources":["src/views/Formwork.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <div style=\"margin: 10px 0\">\r\n            <el-input style=\"width: 200px;margin-left: 5px\" placeholder=\"输入模板类型\" suffix-icon=\"el-icon-search\" v-model=\"name\">\r\n            </el-input>\r\n\r\n            <el-button style=\"margin-left: 5px\" type=\"pimary\"@click=\"load\">搜索</el-button>\r\n        </div>\r\n        <div style=\"margin: 10px 0\">\r\n            <el-button type=\"primary\"@click=\"handleAdd\">新增<i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n            <el-upload action=\"https://jsonplaceholder.typicode.com/posts/\" :show-file-list=\"false\" accept=\"xlsx\" style=\"display: inline-block\">\r\n                <el-button type=\"primary\"style=\"margin-left: 10px\">导入<i class=\"el-icon-download\"></i></el-button>\r\n            </el-upload>\r\n            <el-button type=\"primary\" @click=\"exp\" style=\"margin-left: 10px\">导出<i class=\"el-icon-upload2\"></i></el-button>\r\n        </div>\r\n        <el-table :data=\"tableData\" border stripe>\r\n            <el-table-column prop=\"id\" label=\"ID\" width=\"40\"></el-table-column>\r\n            <el-table-column prop=\"name\" label=\"模板标题\" width=\"140\"></el-table-column>\r\n            <el-table-column prop=\"content\" label=\"模板内容\" >\r\n                <template slot-scope=\"scope\">\r\n                    <el-button @click=\"view(scope.row.content)\" type=\"primary\">查看内容</el-button>\r\n                </template>\r\n            </el-table-column>\r\n\r\n            <el-table-column prop=\"do\" label=\"操作\">\r\n\r\n                <template slot-scope=\"scope\" >\r\n                    <el-button type=\"success\" @click=\"handleUpdate(scope.row)\">编辑</el-button>\r\n                    <el-popconfirm\r\n                            style=\"margin-left:8px \"\r\n                            confirm-button-text='好的'\r\n                            cancel-button-text='不用了'\r\n                            icon=\"el-icon-info\"\r\n                            icon-color=\"red\"\r\n                            title=\"这是一段内容确定删除吗？\"\r\n                            @confirm=\"del(scope.row.id)\"\r\n                    >\r\n                        <el-button type=\"danger\"slot=\"reference\">删除</el-button>\r\n                    </el-popconfirm>\r\n                </template>\r\n\r\n\r\n            </el-table-column>\r\n        </el-table>\r\n        <div style=\"padding: 10px 0\">\r\n            <el-pagination\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    :current-page=\"pageNum\"\r\n                    :page-sizes=\"[5, 10, 15, 20]\"\r\n                    :page-size=\"pageSize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n\r\n\r\n\r\n        </div>\r\n\r\n        <el-dialog  title=\"模板内容\" :visible.sync=\"dialogFormVisible\" width=\"60%\" >\r\n            <el-form label-width=\"80px\" size=\"small\">\r\n                <el-form-item  label=\"模板标题\">\r\n                    <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"模板内容\">\r\n                    <mavon-editor ref=\"md\" v-model=\"form.content\" :ishljs=\"true\"/>\r\n                </el-form-item>\r\n\r\n\r\n\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"save\">保存</el-button>\r\n            </div>\r\n        </el-dialog>\r\n\r\n\r\n        <el-dialog  title=\"模板内容\" :visible.sync=\"viewDialogVis\" width=\"60%\" >\r\n            <el-card>\r\n                <div>\r\n\r\n                    <mavon-editor\r\n                            class=\"md\"\r\n                            :value=\"content\"\r\n                            :subfield=\"false\"\r\n                            :defaultOpen=\"'preview'\"\r\n                            :toolbarsFlag=\"false\"\r\n                            :editable=\"false\"\r\n                            :scrollStyle=\"true\"\r\n                            :ishljs=\"true\"\r\n                    />\r\n                    <div class=\"block\" style=\"margin-left: 350px\">\r\n                        <el-rate v-model=\"value1\"></el-rate>\r\n                    </div>\r\n                </div>\r\n\r\n            </el-card>\r\n\r\n        </el-dialog>\r\n\r\n\r\n    </div>\r\n</template>\r\n<script>\r\n    export default {\r\n        name: \"Formwork\",\r\n        data(){\r\n            return{\r\n                value1: null,\r\n                tableData:[],\r\n                dialogFormVisible:false,\r\n                form:{},\r\n                total:0,\r\n                pageNum:1,\r\n                pageSize:5,\r\n                name:\"\",\r\n                content:'',\r\n                viewDialogVis:false,\r\n                chart: null, // echarts图表实例\r\n            }\r\n        },created() {\r\n            //请求分页数据\r\n            this.load();\r\n\r\n\r\n        },\r\n\r\n        methods:{\r\n\r\n\r\n            load(){\r\n                this.request.get(\"http://localhost:9090/formwork/page\",{\r\n                    params:{\r\n                        pageNum: this.pageNum,\r\n                        pageSize:this.pageSize,\r\n                        name:this.name,\r\n                    }\r\n                }).then(res=>{\r\n                    console.log(res)\r\n                    this.tableData = res.data\r\n                    this.total=res.total\r\n\r\n                })\r\n\r\n            },\r\n            view(content) {\r\n                this.content = content\r\n                this.viewDialogVis = true\r\n            },\r\n            handleAdd(){\r\n                this.dialogFormVisible=true\r\n                this.form={}\r\n            },\r\n            handleUpdate(row){\r\n                this.dialogFormVisible=true\r\n                this.form=row\r\n            },\r\n            save(){\r\n                this.request.post(\"http://localhost:9090/formwork\",this.form).then(res => {\r\n                    if (res) {\r\n                        this.$message.success(\"保存成功\")\r\n                        this.dialogFormVisible = false\r\n                        this.load()\r\n                    } else {\r\n                        this.$message.error(\"保存失败\")\r\n                    }\r\n                })\r\n            },\r\n            del(id){\r\n                this.request.delete(\"http://localhost:9090/formwork/\"+id).then(res => {\r\n                    if (res) {\r\n                        this.$message.success(\"删除成功\")\r\n                        this.dialogFormVisible = false\r\n                        this.load()\r\n                    } else {\r\n                        this.$message.error(\"删除失败\")\r\n                    }\r\n                })\r\n            },\r\n\r\n            handleSizeChange(pageSize){\r\n                this.pageSize=pageSize\r\n                this.load();\r\n            },\r\n            handleCurrentChange(pageNum){\r\n                this.pageNum=pageNum\r\n                this.load();\r\n            },\r\n            exp(){\r\n                window.open(\"http://localhost:9090/user/export\")\r\n            },\r\n\r\n\r\n\r\n\r\n            }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]